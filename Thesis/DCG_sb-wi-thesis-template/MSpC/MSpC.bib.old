@article{
   author = {Vizcaino, J. A. and Cote, R. G. and Csordas, A. and Dianes, J. A. and Fabregat, A. and Foster, J. M. and Griss, J. and Alpi, E. and Birim, M. and Contell, J. and O'Kelly, G. and Schoenegger, A. and Ovelleiro, D. and Perez-Riverol, Y. and Reisinger, F. and Rios, D. and Wang, R. and Hermjakob, H.},
   title = {The PRoteomics IDEntifications (PRIDE) database and associated tools: status in 2013},
   journal = {Nucleic Acids Res.},
   volume = {41},
   number = {Database issue},
   pages = {D1063-9},
   note = {Vizcaino, Juan Antonio
Cote, Richard G
Csordas, Attila
Dianes, Jose A
Fabregat, Antonio
Foster, Joseph M
Griss, Johannes
Alpi, Emanuele
